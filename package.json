{
  "name": "webrtc-voice-system",
  "version": "1.0.0",
  "description": "WebRTC voice chat system with Express and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node src/database/migrate.js",
    "migrate:room-settings": "node src/database/add-room-settings.js",
    "migrate:api-keys": "node src/database/add-api-keys.js",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "seed": "node src/database/seed.js",
    "rooms:clear": "node scripts/clear-rooms.js",
    "rooms:clear:all": "node scripts/clear-rooms.js --all",
    "rooms:clear:force": "node scripts/clear-rooms.js --all --force",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webrtc",
    "express",
    "postgresql",
    "websocket",
    "voice-chat"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^7.0.0",
    "pg": "^8.11.0",
    "sequelize": "^6.35.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
